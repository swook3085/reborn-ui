{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      },
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@modules": "./src/modules",
          "@controller": "./src/controller",
          "@shared": "./src/shared",
          "@components": "./src/components",
          "@interface": "./src/shared/interface"
        }
      }
    ]
  ]
}
